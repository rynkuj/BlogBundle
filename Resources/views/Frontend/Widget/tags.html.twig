<ul id="tags" class="blog-tags-list blog-tags-sidebar-list">
  {% for tag in tags %}
    <li><a href="{{ path('_blog_tag', { 'slug' : tag.slug }) }}" title="{{ tag }}">{{ tag }}</a></li>
  {% else %}
    <li>No hay elementos</li>
  {% endfor %}
  <li><a href="{{ path('_blog_all_tag') }}">{% trans %}All tags{% endtrans %}</a></li>
</ul>
